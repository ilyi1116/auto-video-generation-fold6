{
  "storage": {
    "file": {
      "path": "/vault/data"
    }
  },
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable": true,
      "tls_cert_file": "/vault/ssl/vault.crt",
      "tls_private_key_file": "/vault/ssl/vault.key"
    }
  },
  "seal": {
    "awskms": {
      "region": "us-east-1",
      "kms_key_id": "vault-seal-key"
    }
  },
  "api_addr": "http://localhost:8200",
  "cluster_addr": "http://localhost:8201",
  "ui": true,
  "log_level": "INFO",
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "disable_mlock": true,
  "telemetry": {
    "prometheus_retention_time": "30s",
    "disable_hostname": true
  }
}
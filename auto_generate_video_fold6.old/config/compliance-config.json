{
  "enabled_standards": ["gdpr", "ccpa", "sox", "iso_27001"],
  "organization": {
    "name": "Auto Video Generation Corp",
    "dpo_contact": "dpo@auto-video.com",
    "privacy_officer": "privacy@auto-video.com",
    "compliance_officer": "compliance@auto-video.com",
    "jurisdiction": ["EU", "US", "CA"],
    "business_sectors": ["technology", "media", "entertainment"]
  },
  "gdpr": {
    "enabled": true,
    "data_controller": "Auto Video Generation Corp",
    "dpo_contact": "dpo@auto-video.com",
    "privacy_policy_url": "https://auto-video.com/privacy",
    "cookie_policy_url": "https://auto-video.com/cookies",
    "consent_management": {
      "granular_consent": true,
      "consent_withdrawal_easy": true,
      "consent_proof_storage": true,
      "consent_refresh_months": 24
    },
    "data_subject_rights": {
      "access_request_days": 30,
      "erasure_request_days": 30,
      "rectification_request_days": 30,
      "portability_request_days": 30,
      "automated_processing": true
    },
    "lawful_basis_mapping": {
      "user_registration": "contract",
      "service_provision": "contract", 
      "marketing": "consent",
      "analytics": "legitimate_interests",
      "legal_compliance": "legal_obligation",
      "security": "legitimate_interests"
    },
    "cross_border_transfers": {
      "adequacy_decisions": ["Canada", "Japan", "South Korea"],
      "sccs_in_place": true,
      "bcrs_certified": false,
      "transfer_impact_assessments": true
    }
  },
  "ccpa": {
    "enabled": true,
    "business_contact": "privacy@auto-video.com",
    "opt_out_url": "https://auto-video.com/ccpa-opt-out",
    "do_not_sell_honored": true,
    "consumer_rights": {
      "right_to_know": true,
      "right_to_delete": true,
      "right_to_opt_out": true,
      "right_to_non_discrimination": true
    },
    "personal_information_categories": [
      "identifiers",
      "commercial_information", 
      "internet_activity",
      "geolocation_data",
      "audio_visual_data",
      "professional_information",
      "inferences"
    ],
    "sale_disclosure": {
      "categories_sold": [],
      "categories_not_sold": ["all"],
      "third_party_recipients": []
    }
  },
  "sox": {
    "enabled": true,
    "financial_controls": {
      "access_controls": true,
      "segregation_of_duties": true,
      "authorization_limits": true,
      "audit_trails": true
    },
    "reporting_requirements": {
      "quarterly_reviews": true,
      "annual_assessments": true,
      "management_certifications": true,
      "external_auditor_reviews": true
    }
  },
  "iso_27001": {
    "enabled": true,
    "certification_status": "in_progress",
    "isms_scope": "all_digital_services",
    "risk_assessment_frequency": "annual",
    "controls_implementation": {
      "information_security_policies": true,
      "organization_of_information_security": true,
      "human_resource_security": true,
      "asset_management": true,
      "access_control": true,
      "cryptography": true,
      "physical_environmental_security": true,
      "operations_security": true,
      "communications_security": true,
      "system_acquisition": true,
      "supplier_relationships": true,
      "incident_management": true,
      "business_continuity": true,
      "compliance": true
    }
  },
  "data_classification": {
    "enabled": true,
    "classification_levels": {
      "public": {
        "description": "Information that can be freely shared",
        "examples": ["marketing materials", "press releases"],
        "handling_requirements": []
      },
      "internal": {
        "description": "Information for internal use only",
        "examples": ["internal policies", "org charts"],
        "handling_requirements": ["access_controls", "employee_training"]
      },
      "confidential": {
        "description": "Sensitive business information",
        "examples": ["business plans", "financial data"],
        "handling_requirements": ["access_controls", "encryption", "audit_logging"]
      },
      "restricted": {
        "description": "Highly sensitive information",
        "examples": ["trade secrets", "personal data"],
        "handling_requirements": ["strong_access_controls", "encryption", "audit_logging", "data_loss_prevention"]
      },
      "pii": {
        "description": "Personally identifiable information",
        "examples": ["names", "emails", "addresses"],
        "handling_requirements": ["gdpr_compliance", "encryption", "access_controls", "audit_logging", "retention_policies"]
      }
    },
    "automatic_classification": {
      "enabled": true,
      "ml_models": ["text_classification", "pattern_recognition"],
      "confidence_threshold": 0.8,
      "manual_review_required": true
    }
  },
  "retention": {
    "enabled": true,
    "default_retention_days": 2555,
    "retention_policies": {
      "user_accounts": {
        "data_type": "user_profiles",
        "retention_days": 2555,
        "action": "anonymize",
        "legal_basis": "business_records",
        "exceptions": ["legal_hold", "dispute_resolution"]
      },
      "activity_logs": {
        "data_type": "audit_logs", 
        "retention_days": 2555,
        "action": "archive",
        "legal_basis": "security_compliance",
        "storage_location": "cold_storage"
      },
      "generated_content": {
        "data_type": "ai_generated_videos",
        "retention_days": 1825,
        "action": "delete",
        "legal_basis": "business_operations",
        "user_control": true
      },
      "marketing_data": {
        "data_type": "marketing_analytics",
        "retention_days": 1095,
        "action": "anonymize",
        "legal_basis": "legitimate_interests",
        "consent_based": true
      },
      "payment_data": {
        "data_type": "transaction_records",
        "retention_days": 2555,
        "action": "archive",
        "legal_basis": "legal_obligation",
        "pci_compliance": true
      }
    },
    "automated_enforcement": {
      "enabled": true,
      "schedule": "daily",
      "notification_days_before": 30,
      "grace_period_days": 7,
      "legal_hold_integration": true
    }
  },
  "audit": {
    "enabled": true,
    "audit_scope": "all_systems",
    "real_time_monitoring": true,
    "database": {
      "host": "localhost",
      "port": 5432,
      "database": "compliance_audit",
      "username": "audit_user",
      "password": "${AUDIT_DB_PASSWORD}",
      "ssl_mode": "require",
      "connection_pool_size": 20
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "password": "${REDIS_PASSWORD}",
      "db": 3,
      "ttl_days": 90
    },
    "log_categories": {
      "authentication": true,
      "authorization": true,
      "data_access": true,
      "data_modification": true,
      "system_administration": true,
      "compliance_events": true,
      "security_events": true,
      "privacy_events": true
    },
    "risk_levels": {
      "low": {
        "retention_days": 365,
        "alert_threshold": 1000
      },
      "medium": {
        "retention_days": 1095,
        "alert_threshold": 100
      },
      "high": {
        "retention_days": 2555,
        "alert_threshold": 10
      },
      "critical": {
        "retention_days": 2555,
        "alert_threshold": 1,
        "immediate_notification": true
      }
    },
    "alerts": {
      "enabled": true,
      "notification_methods": ["email", "slack", "sms"],
      "email": {
        "smtp_server": "smtp.company.com",
        "smtp_port": 587,
        "username": "${SMTP_USERNAME}",
        "password": "${SMTP_PASSWORD}",
        "from_address": "compliance-alerts@auto-video.com",
        "recipients": [
          "compliance@auto-video.com",
          "security@auto-video.com",
          "legal@auto-video.com"
        ],
        "escalation_recipients": [
          "ciso@auto-video.com",
          "cto@auto-video.com"
        ]
      },
      "slack": {
        "webhook_url": "${SLACK_COMPLIANCE_WEBHOOK}",
        "channel": "#compliance-alerts",
        "mention_users": ["@compliance-team", "@security-team"]
      }
    }
  },
  "privacy_impact_assessments": {
    "enabled": true,
    "trigger_conditions": [
      "high_risk_processing",
      "new_technology",
      "systematic_monitoring",
      "sensitive_data_processing",
      "large_scale_processing",
      "automated_decision_making"
    ],
    "assessment_template": {
      "data_description": true,
      "processing_purpose": true,
      "lawful_basis": true,
      "necessity_assessment": true,
      "risk_identification": true,
      "mitigation_measures": true,
      "stakeholder_consultation": true,
      "review_schedule": "annual"
    }
  },
  "incident_management": {
    "enabled": true,
    "breach_notification": {
      "internal_notification_hours": 2,
      "dpa_notification_hours": 72,
      "individual_notification_required": true,
      "notification_template": "/templates/breach_notification.html"
    },
    "incident_classification": {
      "low": "minor_policy_violation",
      "medium": "unauthorized_access",
      "high": "data_breach",
      "critical": "major_data_breach"
    },
    "response_team": {
      "incident_commander": "security@auto-video.com",
      "legal_counsel": "legal@auto-video.com", 
      "privacy_officer": "privacy@auto-video.com",
      "communications": "comms@auto-video.com",
      "technical_lead": "engineering@auto-video.com"
    }
  },
  "training": {
    "enabled": true,
    "mandatory_training": {
      "privacy_awareness": {
        "frequency": "annual",
        "duration_hours": 2,
        "passing_score": 80,
        "roles": ["all_employees"]
      },
      "gdpr_training": {
        "frequency": "annual", 
        "duration_hours": 4,
        "passing_score": 85,
        "roles": ["data_processors", "customer_service", "marketing"]
      },
      "security_awareness": {
        "frequency": "quarterly",
        "duration_hours": 1,
        "passing_score": 80,
        "roles": ["all_employees"]
      },
      "compliance_training": {
        "frequency": "annual",
        "duration_hours": 6,
        "passing_score": 90,
        "roles": ["compliance_team", "legal_team", "senior_management"]
      }
    },
    "tracking": {
      "completion_tracking": true,
      "compliance_reporting": true,
      "automated_reminders": true,
      "escalation_enabled": true
    }
  },
  "vendor_management": {
    "enabled": true,
    "due_diligence_requirements": {
      "privacy_assessment": true,
      "security_assessment": true,
      "compliance_certifications": true,
      "data_processing_agreements": true,
      "regular_audits": true
    },
    "approved_vendors": {
      "cloud_providers": ["aws", "gcp", "azure"],
      "ai_services": ["openai", "anthropic", "stability_ai"],
      "analytics": ["google_analytics", "mixpanel"],
      "communication": ["twilio", "sendgrid"]
    },
    "contract_requirements": {
      "data_processing_addendum": true,
      "security_requirements": true,
      "audit_rights": true,
      "breach_notification": true,
      "data_return_deletion": true
    }
  },
  "monitoring": {
    "enabled": true,
    "dashboards": {
      "compliance_overview": true,
      "privacy_metrics": true,
      "audit_summary": true,
      "risk_assessment": true
    },
    "kpis": {
      "gdpr_request_response_time": {
        "target": 25,
        "unit": "days",
        "alert_threshold": 28
      },
      "consent_rate": {
        "target": 85,
        "unit": "percentage",
        "alert_threshold": 80
      },
      "training_completion": {
        "target": 95,
        "unit": "percentage", 
        "alert_threshold": 90
      },
      "audit_finding_resolution": {
        "target": 30,
        "unit": "days",
        "alert_threshold": 45
      }
    },
    "automated_reporting": {
      "enabled": true,
      "daily_summary": true,
      "weekly_reports": true,
      "monthly_dashboards": true,
      "quarterly_assessments": true,
      "annual_compliance_report": true
    }
  },
  "integration": {
    "identity_providers": ["azure_ad", "okta", "ldap"],
    "security_tools": ["siem", "dlp", "iam"],
    "business_systems": ["crm", "erp", "hr_systems"],
    "monitoring_tools": ["prometheus", "grafana", "elk_stack"],
    "ticketing_systems": ["jira", "servicenow"]
  }
}
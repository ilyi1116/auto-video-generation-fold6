{
  "video_generation_count": {
    "type": "counter",
    "description": "Total number of videos generated",
    "labels": ["status", "video_type", "platform", "user_tier"],
    "unit": "count",
    "business_impact": "high",
    "sla_target": "> 1000 per day"
  },
  "user_engagement_rate": {
    "type": "gauge",
    "description": "User engagement rate across all platforms",
    "labels": ["platform", "content_type", "user_segment"],
    "unit": "percentage",
    "business_impact": "high",
    "sla_target": "> 5%"
  },
  "platform_publish_success_rate": {
    "type": "gauge",
    "description": "Success rate of publishing to social media platforms",
    "labels": ["platform", "content_type"],
    "unit": "percentage",
    "business_impact": "critical",
    "sla_target": "> 95%"
  },
  "trend_analysis_accuracy": {
    "type": "gauge",
    "description": "Accuracy of trend analysis predictions",
    "labels": ["source", "trend_type", "time_horizon"],
    "unit": "percentage",
    "business_impact": "high",
    "sla_target": "> 80%"
  },
  "system_availability": {
    "type": "gauge",
    "description": "Overall system availability",
    "labels": ["service", "region"],
    "unit": "percentage",
    "business_impact": "critical",
    "sla_target": "> 99.9%"
  },
  "revenue_per_user": {
    "type": "gauge",
    "description": "Average revenue per user",
    "labels": ["user_tier", "billing_period"],
    "unit": "currency",
    "business_impact": "critical",
    "sla_target": "> $10 per month"
  },
  "content_generation_time": {
    "type": "histogram",
    "description": "Time taken to generate content",
    "labels": ["content_type", "quality_level"],
    "unit": "seconds",
    "business_impact": "medium",
    "sla_target": "< 300 seconds (95th percentile)"
  },
  "user_acquisition_rate": {
    "type": "gauge",
    "description": "Rate of new user acquisition",
    "labels": ["source", "user_tier"],
    "unit": "users_per_day",
    "business_impact": "high",
    "sla_target": "> 100 per day"
  },
  "user_retention_rate": {
    "type": "gauge",
    "description": "User retention rate over different time periods",
    "labels": ["period", "user_cohort"],
    "unit": "percentage",
    "business_impact": "critical",
    "sla_target": "> 70% (30-day retention)"
  },
  "api_usage_per_user": {
    "type": "histogram",
    "description": "API usage per user per day",
    "labels": ["user_tier", "api_category"],
    "unit": "requests_per_day",
    "business_impact": "medium",
    "sla_target": "< 10000 per day (fair usage)"
  },
  "storage_cost_per_video": {
    "type": "gauge",
    "description": "Storage cost per video generated",
    "labels": ["video_quality", "retention_period"],
    "unit": "currency",
    "business_impact": "medium",
    "sla_target": "< $0.10 per video"
  },
  "customer_satisfaction_score": {
    "type": "gauge",
    "description": "Customer satisfaction score from feedback",
    "labels": ["feature", "user_segment"],
    "unit": "score_1_to_10",
    "business_impact": "high",
    "sla_target": "> 8.0"
  },
  "processing_cost_per_request": {
    "type": "gauge",
    "description": "Processing cost per API request",
    "labels": ["service", "request_type"],
    "unit": "currency",
    "business_impact": "medium",
    "sla_target": "< $0.01 per request"
  },
  "social_media_reach": {
    "type": "gauge",
    "description": "Total reach across all social media platforms",
    "labels": ["platform", "content_category"],
    "unit": "impressions",
    "business_impact": "high",
    "sla_target": "> 1M impressions per day"
  },
  "content_moderation_rate": {
    "type": "gauge",
    "description": "Rate of content requiring moderation",
    "labels": ["reason", "severity"],
    "unit": "percentage",
    "business_impact": "high",
    "sla_target": "< 5% flagged content"
  },
  "subscription_conversion_rate": {
    "type": "gauge",
    "description": "Conversion rate from trial to paid subscription",
    "labels": ["plan_type", "user_source"],
    "unit": "percentage",
    "business_impact": "critical",
    "sla_target": "> 15%"
  },
  "feature_usage_rate": {
    "type": "gauge",
    "description": "Usage rate of different features",
    "labels": ["feature", "user_tier"],
    "unit": "percentage",
    "business_impact": "medium",
    "sla_target": "> 40% for core features"
  },
  "support_ticket_resolution_time": {
    "type": "histogram",
    "description": "Time to resolve support tickets",
    "labels": ["priority", "category"],
    "unit": "hours",
    "business_impact": "high",
    "sla_target": "< 24 hours (95th percentile)"
  },
  "churn_risk_score": {
    "type": "gauge",
    "description": "Predictive churn risk score for users",
    "labels": ["user_tier", "risk_level"],
    "unit": "score_0_to_1",
    "business_impact": "critical",
    "sla_target": "< 0.3 average score"
  },
  "content_virality_score": {
    "type": "histogram",
    "description": "Virality score of generated content",
    "labels": ["platform", "content_type"],
    "unit": "score_0_to_100",
    "business_impact": "high",
    "sla_target": "> 20 average score"
  }
}